{
  "name": "reactstarter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "react": "^15.2.0",
    "react-dom": "^15.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "ghooks": "^1.3.2",
    "html-webpack-plugin": "^2.22.0",
    "npm-run-all": "^2.3.0",
    "rimraf": "^2.5.3",
    "webpack": "^2.1.0-beta.15",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-validator": "^2.2.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  },
  "scripts": {
    "lint": "eslint src",
    "build": "webpack --env.dev",
    "build:prod": "webpack --env.prod",
    "prebuild": "npm run clean",
    "prebuild:prod": "npm run clean",
    "clean": "rimraf dist",
    "start": "webpack-dev-server --env.dev --content-base dist",
    "prestart": "npm run clean",
    "validate": "npm-run-all --parallel validate-webpack:* lint",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/achristie/reactstarter.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/achristie/reactstarter/issues"
  },
  "homepage": "https://github.com/achristie/reactstarter#readme"
}
